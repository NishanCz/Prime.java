import java.util.*;
class Prime{
public static void main(String[] args){
Scanner s = new Scanner(system.in);
System.out.println("Enter a number");
int num= s.nextInt();
int flag=1;
for(i=2;i<=num;i++){
if(num%i==0){
flag++;
}
}
if (flag==2){
System.out.println("The entered number is prime");
}
else{
System.out.println("The entered number is not a prime");
}
}
}
